<Project>
  <PropertyGroup>
    <Copyright>Copyright Â© 2020-2021 </Copyright>
    <Authors>Serilog Contrib</Authors>
    <Version>0.0.0.000</Version>
    <PackageProjectUrl>https://github.com/serilog-contrib/Serilog.Enrichers.ActivityTags</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/serilog-contrib/Serilog.Enrichers.ActivityTags/blob/master/LICENSE</PackageLicenseUrl>
    <NoWarn>$(NoWarn);CS1591;xUnit1013</NoWarn>
  </PropertyGroup>
  <PropertyGroup>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All" />
    <None Include="$(SolutionDir)..\icon.png" Pack="true" PackagePath="\"/>
  </ItemGroup>
  <PropertyGroup>
    <PackageIcon>icon.png</PackageIcon>
    <RepositoryUrl>https://github.com/serilog-contrib/Serilog.Enrichers.ActivityTags</RepositoryUrl>
    <EmbedUntrackedSources>true</EmbedUntrackedSources>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
  </PropertyGroup>
</Project>
